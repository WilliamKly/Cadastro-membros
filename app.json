{
  "expo": {
    "name": "Assembleia De Deus",
    "slug": "Igreja",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/deus.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/cf88c75c-9d12-40d1-842a-88a8d07133f8"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "isaelsilva1039"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/deus.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.isaelsilva1039"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "owner": "isaelsilva1039",
    "extra": {
      "eas": {
        "projectId": "4ef4a715-cc22-405e-a27c-65aa21953238"
      }
    }
  }
}
